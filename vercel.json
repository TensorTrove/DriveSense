{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "advanced-dashboard/frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "advanced-dashboard/frontend/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/advanced-dashboard/frontend/$1"
      }
    ]
  }
  